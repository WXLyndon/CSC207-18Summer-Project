<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="150.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.UserStage">
    <children>
        <GridPane layoutX="13.0" layoutY="2.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <TextField fx:id="email" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                <TextField fx:id="accountName" prefHeight="27.0" prefWidth="135.0" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                <Button fx:id="go" mnemonicParsing="false" onAction="#creatingAccount" text="Go for User" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                <Label text="Account Name: " GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Label text="E-mail address:" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <Button mnemonicParsing="false" onAction="#creatingUser" text="Go for Admin" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            </children>
        </GridPane>
        <MenuBar prefHeight="29.0" prefWidth="400.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#logOutForLogIn" text="Return" />
                        <MenuItem mnemonicParsing="false" onAction="#logOutForLogIn" text="Log Out" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
</AnchorPane>
